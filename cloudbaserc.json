{
  "envId": "your-env-id",
  "functionRoot": "./",
  "functions": [
    {
      "name": "json-tools",
      "timeout": 60,
      "envVariables": {},
      "runtime": "Python3.7",
      "memorySize": 256,
      "handler": "app.app"
    }
  ],
  "framework": {
    "plugins": {
      "server": {
        "use": "@cloudbase/framework-plugin-node",
        "inputs": {
          "entry": "./app.py",
          "path": "/",
          "name": "json-tools",
          "wrapExpress": true
        }
      }
    }
  }
}